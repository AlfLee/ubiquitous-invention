banner.charset=UTF-8
server.tomcat.uri-encoding=UTF-8
spring.http.encoding.charset=UTF-8
spring.http.encoding.enabled=true
spring.http.encoding.force=true
spring.message.encoding=UTF-8
##################
##   mybatis配置   ##
##################
mybatis.type-aliases-package=com.telecom.entity
mybatis.table.auto=update
mybatis.model.pack=com.telecom.entity
mybatis.database.type=mysql
##################
##   数据源配置   ##
##################
spring.datasource.driver-class-name = com.mysql.jdbc.Driver
spring.datasource.url = jdbc:mysql://127.0.0.1:3306/test?useUnicode=true&characterEncoding=utf-8&useSSL=false
spring.datasource.username = root
spring.datasource.password = 123456

##################
##   页面配置   ##
##################
spring.mvc.view.prefix=/WEB-INF/jsp/
spring.mvc.view.suffix=.jsp

#Server
server.port=8080
server.jsp-servlet.class-name=org.apache.jasper.servlet.JspServlet

security.basic.enabled=false
management.security.enabled=false

#设置上传文件大小 -1代表不限制
spring.http.multipart.maxFileSize=-1
spring.http.multipart.maxRequestSize=-1

#后台自动打印执行的sql
logging.level.com.telecom.mapper=debug

# tomcat最大线程数，默认为200  
server.tomcat.max-threads=800
# tomcat的URI编码  
server.tomcat.uri-encoding=UTF-8
# 存放Tomcat的日志、Dump等文件的临时文件夹，默认为系统的tmp文件夹（如：C:\Users\Shanhy\AppData\Local\Temp）  
#server.tomcat.basedir=H:/springboot-tomcat-tmp  
# 打开Tomcat的Access日志，并可以设置日志格式的方法：  
#server.tomcat.access-log-enabled=true  
#server.tomcat.access-log-pattern=  
# accesslog目录，默认在basedir/logs  
#server.tomcat.accesslog.directory=  
# 日志文件目录  
logging.path=H:/springboot-tomcat-tmp  
# 日志文件名称，默认为spring.log  
logging.file=myapp.log  

########################
##   设备树根节点配置   ##
########################
euipment.tree.root=所有设备
euipment.tree.id=0
